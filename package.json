{
  "name": "vue-in-express-in-vue",
  "version": "0.0.0",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build --ssr src/main.ts --outDir dist/",
    "start": "node dist/main.js",
    "lint": "eslint ./**/*{.js,.jsx,.ts,.tsx,.vue} -c .eslintrc.js",
    "format": "yarn lint --fix"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.31",
    "@vueuse/core": "^11.1.0",
    "exit-hook": "^4.0.0",
    "express": "^4.21.0",
    "node-cleanup": "^2.1.2",
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "^22.7.4",
    "@types/node-cleanup": "^2.1.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-eslint": "^1.3.0",
    "vue-tsc": "^2.1.6"
  }
}
